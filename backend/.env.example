# App Configuration
APP_TITLE=Car History AI Agent
APP_VERSION=1.0.0
APP_HOST=0.0.0.0
APP_PORT=8000

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["*"]
CORS_ALLOW_HEADERS=["*"]

# API Keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here
