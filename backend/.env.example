NHTSA_API_KEY=your_nhtsa_api_key_here
AI_MOCK_RESPONSE=false

# AI Model Configuration
AI_MODEL=deepseek-chat
AI_BASE_URL=https://api.deepseek.com
AI_MAX_TOKENS=2000

# Logging
LOG_LEVEL=INFO